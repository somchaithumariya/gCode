var name = 'xxx' //newRow[0]
var course = 'xxx' //newRow[4]
sendnotify(name,course)

function sendnotify(name,course){
  var token = 'xxx' // ใส่โทเคนไอดี
  var msg = 'มีคนมาสมัครเข้าใหม่ ชื่อ '+name+ 'หลักสูตร' +course  
  NotifyApp.sendNotify(token,msg)
}


