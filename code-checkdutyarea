function onFormSubmit() {

var form = FormApp.openById('xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx'); //ID Form
var fRes = form.getResponses();

var formResponse = fRes[fRes.length - 1];
var itemResponses = formResponse.getItemResponses();

var suggestion= itemResponses[5].getResponse()
var msg = 'üì£ ‡∏£‡∏≤‡∏¢‡∏á‡∏≤‡∏ô‡∏Å‡∏≤‡∏£‡∏ï‡∏£‡∏ß‡∏à‡πÄ‡∏Ç‡∏ï‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏£‡∏±‡∏ö‡∏ú‡∏¥‡∏î‡∏ä‡∏≠‡∏ö'+
      ' \nüòÄ' + itemResponses[0].getItem().getTitle() + ': ' + itemResponses[0].getResponse() +
      ' \nü§ì' + itemResponses[3].getItem().getTitle() + ': ' + itemResponses[3].getResponse() +
      ' \nüëç' + itemResponses[4].getItem().getTitle() + ': ' + itemResponses[4].getResponse()

  if (suggestion !="") {
    msg=msg+' \n‚úçÔ∏è' + itemResponses[5].getItem().getTitle() + ': ' + itemResponses[5].getResponse()
    }

sendLineNotify(msg);

}

function sendLineNotify(message) {

var token = ["yyyyyyyyyyyyyyyyyyyyyyy"];  //‡∏´‡πâ‡∏≠‡∏á‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô
var options = {
"method": "post",
"payload": "message=" + message,
"headers": {
"Authorization": "Bearer " + token
}
};

UrlFetchApp.fetch("https://notify-api.line.me/api/notify", options);
}

